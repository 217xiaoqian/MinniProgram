<!--components/w-tab-control/w-tab-control.wxml-->
<view class="tab-contorl">
  <block wx:for="{{titles}}" wx:key="{{item}}">
    <view class="tab-item {{currentIndex == index ? 'active' : ''}}" bindtap="handleItemClick" data-index="{{index}}">
      <text>{{item}}</text>
    </view>
  </block>
</view>
